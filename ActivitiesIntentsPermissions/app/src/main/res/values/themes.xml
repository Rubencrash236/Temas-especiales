<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ActivitiesIntentsPermissions" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/khabi_web</item>
        <item name="colorPrimaryVariant">@color/umber</item>
        <item name="colorOnPrimary">@color/rich_black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/khabi_web</item>
        <item name="colorSecondaryVariant">@color/khabi_web</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorOnPrimary</item>
        <!-- Customize your theme here. -->
        <item name="android:background">@color/rich_black</item>
        <item name="android:textColorPrimaryDisableOnly">@color/rich_black</item>
    </style>
    <style name="Theme.ActivitiesIntentsPermissions.NoActionBar">

        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.ActivitiesIntentsPermissions.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>
    <style name="Theme.ActivitiesIntentsPermissions.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>
</resources>